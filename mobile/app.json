{
  "expo": {
    "name": "Audio Castaway",
    "slug": "audio-castaway-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0F0F23"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.audiocastaway.mobile"
    },
    "android": {
      "package": "com.audiocastaway.mobile"
    },
    "plugins": [
      "expo-av"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}